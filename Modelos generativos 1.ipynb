    "man = 1.0 - pei\n",
    "var = 5\n",
    "a = norm.pdf(15, 25, var) * pei\n",
    "b = a + norm.pdf(15, 10, var) * man\n",
    "print ('Posterior' , a / b)\n"
   ]
